<?xml version='1.0' encoding='utf-8'?>
<odoo>
<record id="view_pagos_programados_wizard" model="ir.ui.view">
    <field name="name">pagos.programados.wizard.form</field>
    <field name="model">app.presupuesto.pagos.programados.wizard</field>
    <field name="arch" type="xml">
        <form string="Pagos Programados">
            <group>
                <field name="fecha_desde"/>
                <field name="fecha_hasta"/>
            </group>
            <footer>
                <button string="Generar Reporte" type="object" name="action_generar_reporte" class="btn-primary"/>
                <button string="Cancelar" class="btn-secondary" special="cancel"/>
            </footer>
        </form>
    </field>
</record>

</odoo>
